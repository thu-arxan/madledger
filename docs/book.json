{
    "title": "MadLedger文档",
    "author": "liuyihua <i@liuyihua.com>",
    "language": "zh-hans",
    "root": ".",
    "gitbook": "3.2.3",
    "plugins": [
        "-lunr", "-search", "search-plus", 
        "theme-comscore",
        "copy-code-button",
        "-sharing", "sharing-plus"
    ],
    "pluginsConfig": {
        "sharing": {
           "douban": false,
           "facebook": true,
           "google": true,
           "hatenaBookmark": false,
           "instapaper": false,
           "line": false,
           "linkedin": false,
           "messenger": false,
           "pocket": false,
           "qq": true,
           "qzone": false,
           "stumbleupon": false,
           "twitter": false,
           "viber": false,
           "vk": false,
           "weibo": true,
           "whatsapp": false,
           "all": [
               "facebook", "google", "twitter",
               "weibo", "instapaper", "linkedin",
               "pocket", "stumbleupon"
           ]
       }
    },
    "styles": {
        "website": "styles/website.css"
    }
}
